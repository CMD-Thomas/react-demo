// Typography.styl
h1, h2, h3
  font-family: 'Roboto Slab', serif;
  text-transform uppercase
  
.list 
  margin 0 15px
  padding-top 55px;
  a 
    text-decoration none  

.list__item
  display flex
  justify-content space-between
  flex-wrap wrap
  border-bottom solid 1px #e3e3e3
  padding 1em 0
  list-style-type none
  color #100f2b

.list__item--half
  width 50%
